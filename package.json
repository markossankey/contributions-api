    "@types/node": "^18.15.11",
    "@types/node-cron": "^3.0.7",
    "axios": "^1.3.5",
    "concurrently": "^8.0.1",
    "jest": "^29.5.0",
    "prisma": "^4.12.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.3"
